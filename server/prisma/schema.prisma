generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cancha {
  id     Int     @id
  turnos Turno[]
  nombre String
  info   String?
}

model Turno {
  id     Int    @id @default(autoincrement())
  User     User?   @relation(fields: [userId], references: [id])
  userId   Int?
  Cancha   Cancha? @relation(fields: [canchaId], references: [id])
  canchaId Int?
  Fecha DateTime  

}

model User {
  id     Int     @id @default(autoincrement())
  nombre String?
  email  String  @unique
  turnos Turno[]
}
